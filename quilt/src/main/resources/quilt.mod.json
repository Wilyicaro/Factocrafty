{
  "schema_version": 1,
  "mixin": [
    "factocrafty.mixins.json",
    "factocrafty-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "factocrafty",
    "version": "${version}",
    "name": "Factocrafty",
    "description": "This is an example description! Tell everyone what your mod is about!",
    "authors": ["Wilyicaro"],
    "contact": {
      "sources": "https://github.com/architectury/architectury-templates"
    },
    "license": "MIT",
    "icon": "assets/factocrafty/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "wily.factocrafty.quilt.FactocraftyQuilt"
      ],
      "client_init": [
        "wily.factocrafty.quilt.FactocraftyQuiltClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.2"
      },
      {
        "id": "architectury",
        "version": ">=6.2.43"
      },
      {
        "id": "factory_api",
        "version": ">=0.0.1"
      }
    ]
  }
}