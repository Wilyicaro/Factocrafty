{
  "schemaVersion": 1,
  "id": "factocrafty",
  "version": "${version}",
  "name": "Factocrafty",
  "description": "A mod about creating and reinventing, adding machines that perform the most advanced processes, decorative blocks, power tools, 12 new ores, half of them being naturally generated, and many other usable additions.",
  "authors": [
    "Wilyicaro"
  ],
  "contact": {
    "sources": "${mod_source}"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "wily.factocrafty.fabric.FactocraftyFabric"
    ],
    "client": [
      "wily.factocrafty.fabric.FactocraftyFabricClient"
    ],
    "fabric-datagen": [
      "wily.factocrafty.datagen.FactocraftyDatagen"
    ],
    "jei_mod_plugin": [
      "wily.factocrafty.compat.FactocraftyJeiPlugin"
    ]
  },
  "mixins": [
    "factocrafty.mixins.json",
    "factocrafty-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.20",
    "architectury": ">=9.0.8",
    "factory_api": ">=2.0.2"
  }
}